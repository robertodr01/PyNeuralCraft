{
  "name": "model1",
  "epochs": 500,
  "loss": "mean_squared_error",
  "metrics": ["mean_euclidean_error"],
  "layers": [
    {
      "units": 30,
      "inputs": 10,
      "activation": "sigmoid"
    }, {
      "units": 30,
      "inputs": 30,
      "activation": "sigmoid"
    },
    {
      "units": 3,
      "inputs": 30,
      "activation": "sigmoid"
    }
  ],
  "grid_search": {
    "lr": [0.8, 0.2],
    "momentum": [0.9],
    "Nesterov": [true],
    "kernel_regularizer": [0, 0.0001],
    "bias_regularizer": [0, 0.0001],
    "weights_initializer": ["xavier_uniform_init"]
  }
}